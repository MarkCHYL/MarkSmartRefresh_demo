<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.mark.com.pulltorefresh_demo">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PullableExpandableListViewActivity"/>
        <activity android:name=".activity.PullableGridViewActivity"/>
        <activity android:name=".activity.PullableListViewActivity"/>
        <activity android:name=".activity.PullableScrollViewActivity"/>
        <activity android:name=".activity.PullableTextViewActivity"/>
        <activity android:name=".activity.PullableImageViewActivity"/>
        <activity android:name=".activity.PullableWebViewActivity"/>
    </application>

</manifest>